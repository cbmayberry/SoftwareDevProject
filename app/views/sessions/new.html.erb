<!--sessions/-->
<h1>Sign In</h1>
<%= form_for(:session, url: sign_in_path) do |f| %>
  <p>
  <%= f.label :email %><br>
  <%= f.text_field :email %>
  </p>
  <p>
  <%= f.label :password %><br>
  <%= f.password_field :password %>
  </p>
  <p>
  <%= f.submit 'Log In' %>
  </p>
<% end %>
